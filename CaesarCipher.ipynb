    "def caesar_cipher(msg):#(2) 시저암호 생성 공백은 계속 공백으로\n",
    "    n=random.randint(1,26) #1~25사이의 랜덤 숫자 \n",
    "    for i in range(len(msg)):\n",
    "        if ord(msg[i])==32:#공백\n",
    "            continue\n",
    "        new=ord(msg[i])+n\n",
    "        if new>122 or (new>90 and ord(msg[i])<=90):\n",
    "            new-=26\n",
    "        msg[i]=chr(new)\n",
    "    #newM=''.join(msg)#문자열만들기\n",
    "    decode(msg)\n",
    "    \n",
    "    \n",
    "    \n",
    "##(3) 해독기\n",
    "\n",
    "nltk.download('words') #nltk내부의 영어 단어 사전을 다운로드\n",
    "\n",
    "def count_valid_words(sentence):\n",
    "    sentence=str(sentence)\n",
    "    # nltk에 내장된 영어 단어 사전 가져옴\n",
    "    word_list = set(nltk.corpus.words.words())\n",
    "    # 입력된 문장을 단어 단위로 분리.\n",
    "    words = sentence.split()\n",
    "    # 영어 단어 사전에 있는 단어의 개수를 셈\n",
    "    count = sum(1 for word in words if word.lower() in word_list)\n",
    "    return count #몇개나 일치하는지 리턴\n",
    "\n",
    "\n",
    "times=[]    \n",
    "    \n",
    "def decode(msg):#암호를 해독하는데 말이 되는 걸 찾을 수 있어야함\n",
    "                #문법적으로 옳은 것을 1순위로 하여 출력\n",
    "        \n",
    "    pw=[[0 for i in range(len(msg))] for _ in range (25)]  #복호화 과정에서 사용하는 임시리스트 msg길이만큼 25개 생성\n",
    "    high=-1  #일치단어개수갱신용 \n",
    "    start = time.time()\n",
    "    #############\n",
    "    for i in range(1, 26): #1~25숫자 순서대로 복호테스트\n",
    "        for k in range(len(msg)):\n",
    "            if ord(msg[k])==32:#공백\n",
    "                pw[i-1][k]=chr(32)\n",
    "                continue\n",
    "            new=ord(msg[k])+i\n",
    "            if new>122 or (new>90 and ord(msg[k])<=90):\n",
    "                new-=26\n",
    "            pw[i-1][k]=chr(new)\n",
    "        pw[i-1]=''.join(pw[i-1])\n",
    "        num=count_valid_words(pw[i-1]) ##count_valid_words함수로 몇 개나 철자가 맞는지 셈\n",
    "        pw[i-1]=list(pw[i-1])\n",
    "        if(num > high):  #가장 높은 num을 가진 pw문장을 real에 저장\n",
    "            high=num\n",
    "            real=pw[i-1]\n",
    "    #########         \n",
    "    end=time.time()\n",
    "    sec = (end - start)\n",
    "    result = datetime.timedelta(seconds=sec)\n",
    "    times.append(sec)\n",
    "    print(\"패스워드는 \",''.join(real),\" 걸린 시간: \",result) \n",
    "    return ''.join(real)\n",
    "    \n",
    "    \n",
    "    \n",
    "for i in range(len(sent)):\n",
    "   \n",
    "    msg=sent[i]\n",
    "    msg=list(msg)\n",
    "    caesar_cipher(msg)\n",
    "    \n",
    "print(\"평균시간은 \", sum(times)/(len(sent)))\n",
    "\n"
